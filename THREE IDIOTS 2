#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
    int x1,y1,x2,y2;
    float midx,midy;
    scanf("%d",&x1);
    scanf("%d",&y1);
    scanf("%d",&x2);
    scanf("%d",&y2);
    midx=(x1+x2)/2.0;
    midy=(y1+y2)/2.0;
    printf("Binoy's house is located at (%.1f,%.1f)",midx,midy);    
    return 0;
}
